  },
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "node scripts/run-migrations.js && node scripts/generate-terms-hash.js && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "jest __tests__/api",
    "test:components": "jest __tests__/components",
    "test:mcp": "jest __tests__/mcp-tools",
    "test:services": "jest __tests__/services",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx scripts/seed-data-simple.ts",
    "data:load": "tsx scripts/load-official-data.ts",
    "data:load-sample": "tsx scripts/load-official-data.ts --limit 100",
    "data:load-test": "tsx scripts/load-official-data.ts --limit 10 --dry-run",
    "test:db-connection": "tsx scripts/test-db-connection.ts",
    "diagnose:db": "tsx scripts/diagnose-db.ts",
    "build:production": "NODE_ENV=production node scripts/run-migrations.js && node scripts/generate-terms-hash.js && next build"
  },
